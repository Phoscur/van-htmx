{
  "name": "htmx-van-playground",
  "version": "1.0.0",
  "description": "HTMX Van.JS Playground",
  "type": "module",
  "scripts": {
    "start": "vite",
    "test": "vitest",
    "prettier": "prettier . --write",
    "dev": "tsx watch --ignore dist server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node server"
  },
  "keywords": [
    "htmx",
    "van"
  ],
  "author": "Phoscur",
  "license": "ISC",
  "devDependencies": {
    "@hono/vite-dev-server": "^0.4.1",
    "@types/chance": "^1.1.6",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.32",
    "prettier": "3.2.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  },
  "dependencies": {
    "@hono/node-server": "^1.5.0",
    "body-parser": "^1.20.2",
    "chance": "^1.1.11",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "hono": "^3.12.8",
    "htmx.org": "^1.9.10",
    "mini-van-plate": "^0.5.3",
    "sirv": "^2.0.4",
    "vanjs-core": "^1.2.7"
  }
}
